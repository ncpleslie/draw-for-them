{
  "name": "draw-for-them",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:1-generate": "prisma generate",
    "build:2-migrate": "prisma migrate deploy",
    "build:3-next": "cross-env NODE_ENV=production next build",
    "build:4-server": "tsc --project tsconfig.server.json",
    "build": "run-s build:*",
    "dev:wss": "tsx watch src/server/wss-dev-server.ts --tsconfig tsconfig.server.json",
    "dev:next": "wait-port 3001 && next dev",
    "dev:db-up": "docker compose up -d",
    "dev:db-push": "prisma db push",
    "dev": "run-p dev:*",
    "db-up": "docker compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "prisma migrate dev",
    "db-nuke": "docker compose down --volumes --remove-orphans",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "generate": "prisma generate",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "studio": "prisma studio",
    "start": "cross-env-shell NODE_ENV=production node dist/src/server/wss-prod-server.js",
    "test": "jest --silent=false --json --outputFile=jest-results.json",
    "test:ci": "run-s db-migrate-dev db-seed test"
  },
